{
  "name": "expressservertemplate",
  "version": "1.0.0",
  "description": "template for node server with express",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-scss": "node-sass ./public/scss -o ./public/css",
    "watch-scss": "node-sass --watch ./public/scss -o ./public/css",
    "server": "node app.js",
    "start": "npm-run-all -l -p build-scss watch-scss server "
  },
